{
    "extends": "@rocketseat/eslint-config/react",
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "printWidth": 80,
                "tabWidth": 2,
                "singleQuote": false,
                "doubleQuote": true,
                "trailingComma": "all",
                "arrowParens": "always",
                "semi": true,
                "endOfLine": "auto"
            }
        ]
    }
}